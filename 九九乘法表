i=1
j=1
for i in range(1,10):
    for j in range(1,i+1):
        print(f"{i}x{j}={i*j}",end=" ")
    print("\n")



i=1
j=1
n=0
for i in range(1,10):
    for j in range(i,10):
        print(f"{i}x{j}={i*j}\t",end=" ")
    print("\n")
    for n in range(0,n+1):
        print("\t\t",end=" ")
    n+=1
